doctype xml

rss(
  xmlns:atom = "http://www.w3.org/2005/Atom"
  xmlns:media="http://search.yahoo.com/mrss/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  version = "2.0"
)
  channel
    title!= title
    link= link
    atom:link(
      href = link
      rel = "self"
      type = "application/rss+xml"
    )
    description!= description
    pubDate!= date

    for item in items
      != item
